{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"getId": {
		"prefix": ["getId", "getid"],
		"body": ["const ${1:id} = document.getElementById('${1:id}')"],
		"description": "get Id."
	   },

	"getClass": {
		"prefix": ["getClass", "getclass", "getel", "getEl"],
		"body": ["const ${1:className} = document.querySelector('${1:className}')"],
		"description": "get class."
	},

	"getAllClass": {
		"prefix": ["getAllClasses", "getAllEl", "getallel"],
		"body": ["const ${1:el} = document.querySelectorAll('${1:el}')"],
		"description": "get every sector."
	},

	"cout": {
		"prefix": ["cout", "conso", "con"],
		"body": ["console.log('${1:message}: ' + ${2:2+2})"],
		"description": "logout Values."
	},

	"dialogShow": {
		"prefix": [".show", ".display", ".hs"],
		"body": [".showModal()"],
		"description": "The showModal() method shows a modal dialog."
	},

	"dialogClose": {
		"prefix": [".close", ".exit", ".clo"],
		"body": [".close()"],
		"description": "The close() method closes an HTML <dialog> object."
	}

	

}